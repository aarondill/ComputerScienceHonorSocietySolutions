{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "description": "The metadata for the exercise",
  "type": "object",
  "required": ["createdOn", "description", "name"],
  "properties": {
    "createdOn": {
      "type": "string",
      "format": "date",
      "pattern": "[0-9]{4}-[0-9]{2}-[0-9]{2}",
      "description": "The date the exercise was added to the site. Format: YYYY-MM-DD"
    },
    "description": {
      "type": "string",
      "description": "The description of the exercise"
    },
    "name": {
      "type": "string",
      "description": "The name of the exercise"
    }
  }
}
